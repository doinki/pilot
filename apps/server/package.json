    "clean": "rimraf .turbo && rimraf dist && rimraf node_modules",
    "prepare": "node scripts/prepare.js"
  },
  "dependencies": {
    "@prisma/client": "4.15.0",
    "css-select": "^5.1.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "htmlparser2": "^8.0.2",
    "morgan": "^1.10.0",
    "remeda": "^1.19.0",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.10.0"
  },
  "devDependencies": {
    "@pilot/tsconfig": "workspace:^",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.18",
    "@types/sanitize-html": "^2.9.0",
    "domhandler": "^5.0.3",
    "eslint": "^8.42.0",
    "eslint-config-pilot": "workspace:^",
    "nodemon": "^2.0.22",
    "prisma": "^4.15.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
