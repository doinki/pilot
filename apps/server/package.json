{
  "files": [
    "dist",
    "prisma"
  ],
  "name": "api",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "dev": "nodemon --exec ts-node --files src/app.ts",
    "prebuild": "rimraf dist && pnpm generate",
